<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">


    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>webapp.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>UsersListServlet</servlet-name>
        <servlet-class>webapp.servlets.UsersListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UsersListServlet</servlet-name>
        <url-pattern>/userslist</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>GraphListServlet</servlet-name>
        <servlet-class>webapp.servlets.GraphListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GraphListServlet</servlet-name>
        <url-pattern>/graphlist</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>GetGraphServlet</servlet-name>
        <servlet-class>webapp.servlets.GetGraphServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetGraphServlet</servlet-name>
        <url-pattern>/graph</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>FindRouteServlet</servlet-name>
        <servlet-class>webapp.servlets.FindRouteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FindRouteServlet</servlet-name>
        <url-pattern>/findroute</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>MissionsListServlet</servlet-name>
        <servlet-class>webapp.servlets.MissionsListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MissionsListServlet</servlet-name>
        <url-pattern>/missionlist</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>AddTaskServlet</servlet-name>
        <servlet-class>webapp.servlets.AddTaskServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddTaskServlet</servlet-name>
        <url-pattern>/addtask</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>GetMissionServlet</servlet-name>
        <servlet-class>webapp.servlets.GetMissionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetMissionServlet</servlet-name>
        <url-pattern>/mission</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>TaskInteractionServlet</servlet-name>
        <servlet-class>webapp.servlets.TaskInteractionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TaskInteractionServlet</servlet-name>
        <url-pattern>/missionControl</url-pattern>
    </servlet-mapping>

</web-app>